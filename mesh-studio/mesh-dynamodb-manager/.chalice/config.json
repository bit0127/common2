{
  "version": "2.0",
  "app_name": "mesh-dynamodb-manager",
"stages": {
    "dev": {
      "tag":{
        "Environemnt": "dev",
        "SystemName": "personal-use",
        "Owner": "dash"
      },
      "manage_iam_role": false,
      "iam_role_arn": "arn:aws:iam::626635412172:role/lambda-execute-role",
      "environment_variables":{
        "DYNAMO_DB_REGION": "ap-northeast-1"
      }
    },
    "prod": {
      "tag":{
        "Environemnt": "prod",
        "SystemName": "personal-use",
        "Owner": "dash"
      },
      "manage_iam_role": false,
      "iam_role_arn": "write-your-iam_role-arn",
      "environment_variables":{
        "EC2_REGION": "",
        "SECRET_REGION": ""
      }
      
    }
  }
}
